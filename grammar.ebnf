(* zatím bez dalších operátorů*)
expr=term, expr1;
expr1="+",term,expr1|;
term=factor, term1;
term1="*", list, term1 |;
factor="(", expr , ")" |  list ;

nonZeroDigit = "1"|"2"|"3"|"4"|"5"|"6"|"7"|"8"|"9";
digit = nonZeroDigit | "0";
int = (["-"], nonZeroDigit, {digit});
binary = binaryStart, binaryDigit, {binaryDigit};
binaryStart = "0b" | "0B";
binaryDigit = "0" | "1";
list = int | binary | "a";
syntax =expr;